@model project_web.Models.TipoEscenario

@{
    ViewData["Title"] = "Details";
}
<div class="containerAr">


<div>
    <h2>Detalles tipo escenario</h2>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Descripcion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descripcion)
        </dd>
        <dt class = "col-sm-2">
            Creado el
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedAt)
        </dd>
        <dt class = "col-sm-2">
            Creado por
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedBy)
        </dd>
        <dt class = "col-sm-2">
            Actualizado el
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UpdatedAt)
        </dd>
        <dt class = "col-sm-2">
           Actualizado por
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UpdatedBy)
        </dd>
        <dt class = "col-sm-2">
            Activo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Active)
        </dd>
        <dt class = "col-sm-2">
            Id escenario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdEscenarioNavigation.Id)
        </dd>
    </dl>
</div>
<div>
        <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.Id">Editar</a>
        <a class="btn btn-secondary" asp-action="Index">Volver</a>
</div>
</div>